{
  "expo": {
    "name": "Doodle",
    "slug": "doodle-wallpaper-studio",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "doodle",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFDF9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doodle.wallpaper"
    },
    "android": {
      "package": "com.doodle.wallpaper",
      "adaptiveIcon": {
        "backgroundColor": "#FFDE59",
        "foregroundImage": "./src/assets/icon.png"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
